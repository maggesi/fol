/* ========================================================================= */
/* Interpetation of first order logic in the language of arith3.             */
/* ========================================================================= */

sig interp.

accum_sig fol, arith3.

/* ------------------------------------------------------------------------- */
/* Language of terms.                                                        */
/* ------------------------------------------------------------------------- */

type zero, one, two term.
type add term -> term -> term.
type mul term -> term -> term.

/* ------------------------------------------------------------------------- */
/* Relations.                                                                */
/* ------------------------------------------------------------------------- */

type eq term -> term -> form.

end
